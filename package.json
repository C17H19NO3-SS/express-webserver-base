{
  "name": "@synchjs/ewb",
  "version": "1.0.17",
  "description": "Express Web Server Base - A powerful, type-safe Express.js wrapper with integrated Swagger, CORS, Rate Limit, JWT and Decorators.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./hmr": {
      "types": "./dist/hmr.d.ts",
      "import": "./dist/hmr.js",
      "require": "./dist/hmr.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "express",
    "bun",
    "typescript",
    "decorators",
    "swagger",
    "jwt",
    "auth",
    "rest",
    "api"
  ],
  "author": "SynchJS",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@swc/core": "^1.15.11",
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "autoprefixer": "^10.4.24",
    "openapi-types": "^12.1.3",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "supertest": "^7.2.2",
    "tailwindcss": "^4.2.0",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "boxen": "^8.0.1",
    "bun-plugin-tailwind": "^0.1.2",
    "chalk": "^5.6.2",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.3",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "swagger-ui-express": "^5.0.1"
  },
  "scripts": {
    "dev": "bun run --watch example/index.ts",
    "test": "bun test",
    "build": "tsup src/index.ts src/hmr.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "bun run build && bun test"
  }
}
